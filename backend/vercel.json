{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/reminders",
            "schedule": "0 0 * * *"
        },
        {
            "path": "/api/cron/reminders",
            "schedule": "0 9 * * *"
        }
    ]
}